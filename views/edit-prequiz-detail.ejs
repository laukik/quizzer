 <html>
        <head>
            <meta charset="utf-8"> 
            <link rel= 'stylesheet' href='bootstrap/css/bootstrap.min.css'  type='text/css'/> 
            <script src='alldt.js'></script>
        </head>
    <body background="img/moulin.png">
        <div class="container">
            <div class="row">
                <div class="col-lg-12">
                    <nav class="navbar navbar-inverse navbar-fixed-top" role="navigation">
                        <div class="navbar-header">
                            <h3 style="color:white" >TestWiz</h3>
                        </div>
                       <div class="collapse navbar-collapse" id="bs-example-navbar-collapse-1">
                          <form class="navbar-form navbar-right" action='/logout'>
                                <div class="btn-group">
                                  <button type="button" class="btn btn-inverse dropdown-toggle" data-toggle="dropdown" style="background: rgba(54, 25, 25, 0);color:#fff">
                                    <span class="glyphicon glyphicon-user"></span> <%= title %> 
                                    <span class="caret"></span>
                                  </button>
                                  <ul class="dropdown-menu">
                                    <li ><a href="/home" ><span class="glyphicon glyphicon-home"></span> Home</a></li>
                                    <li><a href="/cpasswd?user=<%= title %>" ><span class="glyphicon glyphicon-edit"></span> Change Password</a></li>
                                    <li><a href="/login"><span class="glyphicon glyphicon-wrench"></span> Change email address</a></li>
                                  </ul>
                                </div>
                                <button type="submit" class="btn navbar-btn btn-inverse" style="background: rgba(54, 25, 25, 0);color:#fff"><span class="glyphicon glyphicon-log-out"></span> logout</button>
                          </form>
                       </div>
                        
                    </nav>
                </div>
            </div>
            </br>
            </br>
            </br></br></br>
            
            <div class="row">
                <div class="col-lg-4">
                    <button type="submit" class="btn-inverse form-control"  style="margin-bottom:22px;height:42px">
                        <span class="glyphicon glyphicon-exclamation-sign">
                        </span> QUIZ-ID :: <%= Qid %>
                    </button>
                </div>
                <div class="col-lg-4">
                </div>
                <div class="col-lg-4"></div>
            </div>
            </br>
            </br>
            <div class="row">
                <div class="col-lg-12">
                    <form class="form" role="form" action="/edit-quizdetail" method="post" id="form">
                        <div class="col-lg-4">
                            <p style="color:green;margin-left:27%;">SCHEDULE DATA</p>
                            <div class="input-group">
                                <span class="input-group-btn">
                                    <a href="javascript:NewCal('acttime','ddmmyyyy',true,24)">
                        <button class="btn-lg btn-success" type="button" onclick='javascript:NewCal('acttime','ddmmyyyy',true,12)' > <span class="glyphicon glyphicon-time"></span>
                                        </button>
                                    </a>
                                </span>
                                <input type="text" class="form-control" name="acttime" id="acttime" style="height:42px;width:250px;padding:10px;" placeholder="Select activation date and time" maxlength="25" size="25" value="<%= Qd[1] %> <%= Qd[2] %>" >
                            </div>
                            <div class="input-group">
                                <span class="input-group-btn" >
                                    <a href="javascript:NewCal('endtime','ddmmyyyy',true,24)">
                                        <button class="btn-lg btn-success" style="margin-top: 10px" type="button" onclick='javascript:NewCal('endtime','ddmmyyyy',true,12)' >
                                            <span class="glyphicon glyphicon-time"></span>
                                        </button>
                                    </a>
                                </span>
                                <input type="text" class="form-control" style="height:42px;width:250px;margin-top: 10px" name="endtime" id="endtime" placeholder="Select completion date and time"maxlength="25" size="25" value="<%= Qd[3] %> <%= Qd[4]%>">
                            </div>
                        </div>
                        <div class="col-lg-4">
                            <p style="color:green;margin-left:20%;">QUIZ PASSWORD</p>
                            <input type="password" class="form-control" name="password" id="password" placeholder="password"
                            style="height:42px;width:250px;margin-bottom:10px;" value=<%= Qd[0] %> >
                            <input type="password" class="form-control" name="xpassword" id="xpassword" placeholder="confirm password" style="height:42px;width:250px;margin-top: 10px;margin-bottom:10px;" value=<%= Qd[0] %> >
                        </div>
                        <div class="row" >
                            <div class="col-lg-3"></div>
                            <div class="col-lg-6">
                                <p style="color:green;margin-top:60px;margin-left: 35%;">APPLICANT PASSWORD</p>
                                <input type="password" class="form-control" name="password2" id="password2" placeholder="password for applicant" style="height:42px;width:100%;margin-bottom:10px;" value=<%= Qd[6] %>>
                                <input type="password" class="form-control" name="xpassword2" id="xpassword2" placeholder="confirm password" style="height:42px;width:100%;margin-top: 10px;margin-bottom:10px;" value=<%= Qd[6] %>>
                            </div>
                            <div class="col-lg-3">

                            </div>
                        </div>
                        <div class="row">
                            <div class="col-lg-3"></div>
                            <div class="col-lg-6">
                                <button class="btn-lg btn-success form-control" type="button" id="frmsub" style="height:42px;width:100%;">
                                    <span class="glyphicon glyphicon-check">
                                </button>        
                            </div>
                            <div class="col-lg-3"></div>
                        </div>
                    </form>
                </div>
            </div>
        </div>
        </br>
        </br></br></br></br></br></br></br>
        <div class="container marketing" >
            <div class="row"  style="color:white">
                <div class="col-lg-2">
                    <img src="icons/cloud-upload-wh.png" class="img-rounded image" id="image" onmouseover="rotation(this);" style="height:140px;width:140px">
                    <h2> Upload Questions </h2>
                    <p> Create Quiz by,Providing Questions to us with their solutions and we will conduct it for you</p>
                </div>
                <div class="col-lg-2">
                    <img src="icons/list-ol-wh.png" class="img-rounded" onmouseover="rotation(this);" style="height:140px;width:140px">
                    <h2> Versetility Of Questions </h2>
                    <p> Various kind of questions like MCQ, Subjective, questions with images and many more all under the same roof</p>
                </div>
                <div class="col-lg-2">
                    <img src="icons/calendar-wh.png" class="img-rounded" onmouseover="rotation(this);" style="height:140px;width:140px">
                    <h2> Fix a Schedule </h2>
                    <p> Provide us a date and time to propell your Quiz</p>
                </div>
                <div class="col-lg-2">
                    <img src="icons/rocket-wh.png" class="img-rounded" onmouseover="rotation(this);" style="height:140px;width:140px">
                    <h2> Launch </h2>
                    <p>  We will Launch it, on schedule provided by you...  </p>    
                </div><div class="col-lg-2">
                    <img src="icons/bar-chart-o-wh.png" class="img-rounded" onmouseover="rotation(this);" style="height:140px;width:140px">
                    <h2> Get The Result   </h2>
                    <p> Get the complete statics of evalution </p>
                </div>
                <div class="col-lg-2">
                    <img src="icons/thumbs-o-up-wh.png" class="img-rounded"  onmouseover="rotation(this);" style="height:140px;width:140px">
                    <h2> Like Us </h2>
                    <p> Like our official page </p>
                </div>
            </div>
        </div>
    </body>
    <script>
        var $j = jQuery.noConflict();
        var rotation = function (ele){
           $j(ele).rotate({
              angle:0, 
              animateTo:360, 
              callback: rotation,
              easing: function (x,t,b,c,d){        // t: current time, b: begInnIng value, c: change In value, d: duration
                  return c*(t/d)+b;
              }
           });
        }
        $j(function(){
            $j('#frmsub').click(function(){
                if( $j('#password').val() === $j('#xpassword').val() ){
                    if( $j('#password2').val() === $j('#xpassword2').val() ){
                        $j('#form').submit();
                    }else{
                        $j('#password2').val("");
                        $j('#xpassword2').val("");

                    }
                } else{
                    $j('#password').val("");
                    $j('#xpassword').val("");
                }
            });
        });
    </script>
	
</html>